{
  "version": 2,
  "public": true,
  "rewrites": [
    {
      "source": "/ru/(.*)",
      "destination": "/ru/index.html"
    },
    {
      "source": "/en/(.*)",
      "destination": "/en/index.html"
    },
    {
      "source": "/",
      "destination": "/ru/index.html"
    }
  ],
  "cleanUrls": true
}
